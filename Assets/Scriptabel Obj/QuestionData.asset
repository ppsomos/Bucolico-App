      \u0395\u03AF\u03BD\u03B1\u03B9 \u03CC\u03BD\u03C4\u03C9\u03C2 \u03B3\u03B9\u03B1
      \u03B1\u03C5\u03C4\u03AE\u03BD; \u0395\u03AF\u03BD\u03B1\u03B9 \u03C9\u03C1\u03B1\u03AF\u03BF
      \u03BD\u03B1 \u03BF\u03BD\u03B5\u03B9\u03C1\u03B5\u03CD\u03B5\u03C4\u03B1\u03B9
