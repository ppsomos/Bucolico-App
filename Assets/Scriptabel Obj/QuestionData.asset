      \u03A0\u03CE\u03C2 \u03B8\u03B1 \u03B5\u03BD\u03B5\u03C1\u03B3\u03BF\u03CD\u03C3\u03B1\u03C4\u03B5;"
    - "\u0395\u03AF\u03C3\u03C4\u03B5 \u03C0\u03BF\u03BB\u03CD \u03BA\u03B1\u03BB\u03BF\u03AF
      \u03C3\u03C4\u03B9\u03C2 \u03B3\u03BB\u03CE\u03C3\u03C3\u03B5\u03C2 \u03BA\u03B1\u03B9
      \u03C3\u03B1\u03C2 \u03B1\u03C1\u03AD\u03C3\u03B5\u03B9 \u03BD\u03B1 \u03C0\u03B1\u03AF\u03B6\u03B5\u03C4\u03B5
      \u03BA\u03B1\u03B9 \u03BD\u03B1 \u03B4\u03B9\u03B1\u03C3\u03BA\u03B5\u03B4\u03AC\u03B6\u03B5\u03C4\u03B5
      \u03BC\u03B1\u03B8\u03B1\u03AF\u03BD\u03BF\u03BD\u03C4\u03B1\u03C2 \u03BC\u03B9\u03B1
