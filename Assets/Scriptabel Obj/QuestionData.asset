      \u0391\u03BB\u03BB\u03AC \u03B1\u03C2 \u03B5\u03AF\u03BC\u03B1\u03C3\u03C4\u03B5
      \u03C7\u03B1\u03BB\u03B1\u03C1\u03BF\u03AF: \u03C4\u03AF\u03C0\u03BF\u03C4\u03B1
      \u03B4\u03B5\u03BD \u03B8\u03B1 \u03C3\u03C5\u03BC\u03B2\u03B5\u03AF \u03B7
      \u03B5\u03C4\u03B1\u03B9\u03C1\u03B5\u03AF\u03B1 \u03B8\u03B1 \u03B1\u03BD\u03B1\u03BA\u03AC\u03BC\u03C8\u03B5\u03B9
      \u03B1\u03C1\u03B3\u03AC \u03AE \u03B3\u03C1\u03AE\u03B3\u03BF\u03C1\u03B1.
      \u038C\u03BB\u03BF\u03B9 \u03BA\u03AC\u03BD\u03BF\u03C5\u03BD \u03BB\u03AC\u03B8\u03B7
      \u03BA\u03B1\u03B9 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03B1\u03C0\u03BB\u03AC
      \u03BD\u03B1 \u03C3\u03C5\u03BC\u03B2\u03B5\u03AF."
    - "\u0388\u03BD\u03B1\u03C2 \u03C4\u03B1\u03BA\u03C4\u03B9\u03BA\u03CC\u03C2
      \u03C0\u03B5\u03BB\u03AC\u03C4\u03B7\u03C2 \u03C4\u03B7\u03C2 \u03B5\u03C4\u03B1\u03B9\u03C1\u03B5\u03AF\u03B1\u03C2
      \u03C3\u03B1\u03C2 \u03B6\u03B7\u03C4\u03AC \u03AD\u03BA\u03C0\u03C4\u03C9\u03C3\u03B7.
      \u0395\u03C3\u03B5\u03AF\u03C2 \u03CC\u03BC\u03C9\u03C2 \u03B5\u03AF\u03C3\u03C4\u03B5
      \u03BC\u03CC\u03BD\u03BF\u03C2 \u03BA\u03B1\u03B9 \u03BA\u03B1\u03C4\u03CE\u03C4\u03B5\u03C1\u03BF\u03C2
      \u03C5\u03C0\u03AC\u03BB\u03BB\u03B7\u03BB\u03BF\u03C2 \u03B4\u03B5\u03BD \u03C5\u03C0\u03AC\u03C1\u03C7\u03B5\u03B9
      \u03BA\u03B1\u03BD\u03AD\u03BD\u03B1\u03C2 \u03AC\u03BB\u03BB\u03BF\u03C2.
      \u039E\u03AD\u03C1\u03B5\u03C4\u03B5 \u03C4\u03B9 \u03B8\u03B1 \u03B1\u03BD\u03B1\u03B6\u03B7\u03C4\u03AE\u03C3\u03B5\u03B9
      \u03BF \u03C0\u03B5\u03BB\u03AC\u03C4\u03B7\u03C2 \u03C3\u03B1\u03C2 \u03B3\u03B9\u03B1
      \u03C4\u03B7\u03BD \u03AF\u03B4\u03B9\u03B1 \u03C5\u03C0\u03B7\u03C1\u03B5\u03C3\u03AF\u03B1
      \u03B1\u03BB\u03BB\u03BF\u03CD \u03C1\u03C9\u03C4\u03CE\u03BD\u03C4\u03B1\u03C2
      \u03AD\u03BD\u03B1\u03BD \u03B1\u03BD\u03C4\u03B1\u03B3\u03C9\u03BD\u03B9\u03C3\u03C4\u03AE
      \u03C3\u03B1\u03C2. \u0395\u03C3\u03B5\u03AF\u03C2 \u2013 \u03C0\u03BF\u03BB\u03CD
      \u03BD\u03B5\u03C5\u03C1\u03B9\u03BA\u03AC \u2013 \u03B5\u03B3\u03BA\u03C1\u03AF\u03BD\u03B5\u03C4\u03B5
      \u03C4\u03B7\u03BD \u03AD\u03BA\u03C0\u03C4\u03C9\u03C3\u03B7 \u03B3\u03B9\u03B1
      \u03BD\u03B1 \u03BA\u03B5\u03C1\u03B4\u03AF\u03C3\u03B5\u03C4\u03B5 \u03C4\u03BF\u03C5\u03C2
      \u03B1\u03BD\u03C4\u03B1\u03B3\u03C9\u03BD\u03B9\u03C3\u03C4\u03AD\u03C2 \u03C3\u03B1\u03C2
      \u03BA\u03AC\u03BD\u03BF\u03BD\u03C4\u03B1\u03C2 \u03C4\u03BF\u03BD \u03C4\u03B1\u03BA\u03C4\u03B9\u03BA\u03CC
      \u03C3\u03B1\u03C2 \u03C0\u03B5\u03BB\u03AC\u03C4\u03B7 \u03B5\u03C5\u03C7\u03B1\u03C1\u03B9\u03C3\u03C4\u03B7\u03BC\u03AD\u03BD\u03BF.
      \u0391\u03C0\u03BB\u03CE\u03C2 \u03B5\u03BB\u03C0\u03AF\u03B6\u03B5\u03C4\u03B5
      \u03CC\u03C4\u03B9 \u03BF\u03B9 \u03B4\u03B9\u03B5\u03C5\u03B8\u03C5\u03BD\u03C4\u03AD\u03C2
      \u03C3\u03B1\u03C2 \u03B4\u03B5\u03BD \u03B8\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
      \u03B8\u03C5\u03BC\u03C9\u03BC\u03AD\u03BD\u03BF\u03B9 \u03BC\u03B1\u03B6\u03AF
      \u03C3\u03B1\u03C2."
    - "\u039A\u03B1\u03C4\u03B1\u03C6\u03AD\u03C1\u03B1\u03C4\u03B5 \u03BD\u03B1
      \u03B2\u03C1\u03B5\u03AF\u03C4\u03B5 \u03BC\u03B9\u03B1 \u03C0\u03BF\u03BB\u03CD
      \u03BA\u03B1\u03BB\u03AE \u03B4\u03BF\u03C5\u03BB\u03B5\u03B9\u03AC \u03C0\u03BF\u03C5
      \u03C3\u03B1\u03C2 \u03B1\u03C1\u03AD\u03C3\u03B5\u03B9. \u0391\u03BB\u03BB\u03AC
      \u03B5\u03BE\u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03B5\u03AF\u03C4\u03B5
      \u03BD\u03B1 \u03BC\u03B5\u03C4\u03B1\u03BD\u03B9\u03CE\u03BD\u03B5\u03C4\u03B5:
      \u03B8\u03AD\u03BB\u03B5\u03C4\u03B5 \u03C0\u03C1\u03B1\u03B3\u03BC\u03B1\u03C4\u03B9\u03BA\u03AC
      \u03BD\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
      \u03BC\u03B1\u03B8\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BD\u03B1 \u03C3\u03C5\u03BD\u03B5\u03C7\u03AF\u03C3\u03B5\u03C4\u03B5
      \u03C4\u03B7\u03BD \u03B5\u03BA\u03C0\u03B1\u03AF\u03B4\u03B5\u03C5\u03C3\u03AE
      \u03C3\u03B1\u03C2 \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C0\u03AC\u03C1\u03B5\u03C4\u03B5
      \u03AD\u03BD\u03B1 \u03C0\u03BB\u03AE\u03C1\u03B5\u03C2 \u03C0\u03C4\u03C5\u03C7\u03AF\u03BF.
      \u03A4\u03B1 \u03C7\u03C1\u03AE\u03BC\u03B1\u03C4\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
      \u03C0\u03B5\u03C1\u03B9\u03BF\u03C1\u03B9\u03C3\u03BC\u03AD\u03BD\u03B1 \u03B4\u03B5\u03BD
      \u03C5\u03C0\u03AC\u03C1\u03C7\u03B5\u03B9 \u03C0\u03BF\u03BB\u03CD\u03C2 \u03B5\u03BB\u03B5\u03CD\u03B8\u03B5\u03C1\u03BF\u03C2
      \u03C7\u03C1\u03CC\u03BD\u03BF\u03C2. \u0391\u03BB\u03BB\u03AC \u03B8\u03C5\u03C3\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2
      \u03C4\u03B1 \u03C7\u03CC\u03BC\u03C0\u03B9 \u03C4\u03BF\u03C5\u03C2 \u03C6\u03AF\u03BB\u03BF\u03C5\u03C2
      \u03BA\u03B1\u03B9 \u03C4\u03B7\u03BD \u03BF\u03B9\u03BA\u03BF\u03B3\u03AD\u03BD\u03B5\u03B9\u03AC
      \u03C3\u03B1\u03C2 \u03C4\u03B1 \u03C7\u03C1\u03AE\u03BC\u03B1\u03C4\u03B1
      \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03B5\u03BB\u03B9\u03BA\u03AC \u03B1\u03C1\u03BA\u03B5\u03C4\u03AC.
      \u0398\u03AD\u03BB\u03B5\u03C4\u03B5 \u03C0\u03C1\u03B1\u03B3\u03BC\u03B1\u03C4\u03B9\u03BA\u03AC
      \u03BD\u03B1 \u03B5\u03C0\u03B5\u03BD\u03B4\u03CD\u03C3\u03B5\u03C4\u03B5 \u03C3\u03C4\u03BF\u03BD
      \u03B5\u03B1\u03C5\u03C4\u03CC \u03C3\u03B1\u03C2 \u03BA\u03B1\u03B9 \u03C3\u03C4\u03B9\u03C2
      \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03AD\u03C2 \u03C3\u03B1\u03C2."
    - "\u0393\u03BD\u03C9\u03C1\u03AF\u03B6\u03B5\u03C4\u03B5 \u03CC\u03C4\u03B9
      \u03AD\u03C1\u03C7\u03BF\u03BD\u03C4\u03B1\u03B9 \u03B2\u03C1\u03B1\u03B2\u03B5\u03AF\u03B1
      \u03B5\u03C1\u03B3\u03B1\u03B6\u03BF\u03BC\u03AD\u03BD\u03C9\u03BD \u03BA\u03B1\u03B9
      \u03B5\u03C0\u03AF\u03C3\u03B7\u03BC\u03B7 \u03B1\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7.
      \u0391\u03C5\u03C4\u03CC \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C5\u03C0\u03AD\u03C1\u03BF\u03C7\u03BF:
      \u03C4\u03B1 \u03BF\u03BD\u03CC\u03BC\u03B1\u03C4\u03B1 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
      \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B4\u03B7\u03BC\u03CC\u03C3\u03B9\u03B1
      \u03BA\u03B1\u03B9 \u03AD\u03C4\u03C3\u03B9 \u03B7 \u03C0\u03BF\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1
      \u03C4\u03C9\u03BD \u03B2\u03C1\u03B1\u03B2\u03B5\u03AF\u03C9\u03BD. \u0398\u03B1
      \u03B4\u03CE\u03C3\u03B5\u03B9 \u03BA\u03AF\u03BD\u03B7\u03C4\u03C1\u03BF \u03C3\u03B5
      \u03CC\u03C3\u03BF\u03C5\u03C2 \u03B4\u03B5\u03BD \u03B8\u03B1 \u03BB\u03AC\u03B2\u03BF\u03C5\u03BD
      \u03BA\u03B1\u03BC\u03AF\u03B1 \u03B1\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7
      \u03BA\u03B1\u03B9 \u03BC\u03C0\u03CC\u03BD\u03BF\u03C5\u03C2."
    - "\u039D\u03BF\u03BC\u03AF\u03B6\u03B5\u03C4\u03B5 \u03CC\u03C4\u03B9 \u03B5\u03AF\u03BD\u03B1\u03B9
      \u03BA\u03B1\u03B9\u03C1\u03CC\u03C2 \u03BD\u03B1 \u03B1\u03BD\u03B1\u03B6\u03B7\u03C4\u03AE\u03C3\u03B5\u03C4\u03B5
      \u03BA\u03B1\u03B9\u03BD\u03BF\u03C4\u03CC\u03BC\u03B5\u03C2 \u03C0\u03C1\u03B1\u03BA\u03C4\u03B9\u03BA\u03AD\u03C2
      \u03BA\u03B1\u03B9 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B5\u03C2
      \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03B5\u03C4\u03B1\u03B9\u03C1\u03B5\u03AF\u03B1
      \u03C3\u03B1\u03C2. \u0391\u03C1\u03C7\u03AF\u03B6\u03B5\u03C4\u03B5 \u03BD\u03B1
      \u03C0\u03B1\u03C1\u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03B5\u03AF\u03C4\u03B5
      \u03C4\u03BF\u03C5\u03C2 \u03C0\u03B5\u03BB\u03AC\u03C4\u03B5\u03C2 \u03BA\u03B1\u03B9
      \u03BD\u03B1 \u03C3\u03C5\u03BB\u03BB\u03AD\u03B3\u03B5\u03C4\u03B5 \u03C4\u03B1
      \u03C3\u03C7\u03CC\u03BB\u03B9\u03AC \u03C4\u03BF\u03C5\u03C2: \u03C4\u03B9
      \u03BA\u03B1\u03BB\u03CD\u03C4\u03B5\u03C1\u03BF \u03B1\u03C0\u03CC \u03C4\u03B9\u03C2
      \u03C3\u03BA\u03AD\u03C8\u03B5\u03B9\u03C2 \u03C4\u03BF\u03C5 \u03C4\u03B5\u03BB\u03B9\u03BA\u03BF\u03CD
      \u03C7\u03C1\u03AE\u03C3\u03C4\u03B7;"
    - "\u0397 \u03B5\u03C4\u03B1\u03B9\u03C1\u03B5\u03AF\u03B1 \u03C3\u03B1\u03C2
      \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
      \u03B5\u03BD\u03AE\u03BC\u03B5\u03C1\u03B7 \u03C0\u03B1\u03C1\u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03CE\u03BD\u03C4\u03B1\u03C2
      \u03C4\u03BF\u03BD \u03B1\u03BD\u03C4\u03B1\u03B3\u03C9\u03BD\u03B9\u03C3\u03BC\u03CC
      \u03C4\u03B7\u03C2. \u0395\u03AF\u03C3\u03C4\u03B5 \u03C3\u03AF\u03B3\u03BF\u03C5\u03C1\u03BF\u03C2
      \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03C0\u03BF\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1
      \u03C4\u03B7\u03C2 \u03C0\u03C1\u03BF\u03C3\u03C0\u03AC\u03B8\u03B5\u03B9\u03AC\u03C2
      \u03C3\u03B1\u03C2 \u03C8\u03AC\u03C7\u03BD\u03B5\u03C4\u03B5 \u03C3\u03C5\u03BD\u03AD\u03C7\u03B5\u03B9\u03B1
      \u03B3\u03B9\u03B1 \u03C0\u03BB\u03B7\u03C1\u03BF\u03C6\u03BF\u03C1\u03AF\u03B5\u03C2.
      \u0395\u03BE\u03AC\u03BB\u03BB\u03BF\u03C5 \u03B7 \u03BA\u03B1\u03B9\u03BD\u03BF\u03C4\u03BF\u03BC\u03AF\u03B1
      \u03BA\u03B1\u03B9 \u03B7 \u03B1\u03BD\u03C4\u03B1\u03B3\u03C9\u03BD\u03B9\u03C3\u03C4\u03B9\u03BA\u03CC\u03C4\u03B7\u03C4\u03B1
      \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BA\u03AC\u03C4\u03B9 \u03C0\u03BF\u03C5
      \u03BA\u03AC\u03B8\u03B5 \u03B5\u03C1\u03B3\u03B1\u03B6\u03CC\u03BC\u03B5\u03BD\u03BF\u03C2
      \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03BA\u03B1\u03BB\u03BB\u03B9\u03B5\u03C1\u03B3\u03AE\u03C3\u03B5\u03B9
      \u03BC\u03CC\u03BD\u03BF\u03C2 \u03C4\u03BF\u03C5."
    - "\u03A7\u03AC\u03C1\u03B7 \u03C3\u03C4\u03B9\u03C2 \u03C0\u03C1\u03BF\u03C3\u03C9\u03C0\u03B9\u03BA\u03AD\u03C2
      \u03C3\u03B1\u03C2 \u03B5\u03C0\u03B1\u03C6\u03AD\u03C2 \u03C6\u03AD\u03C1\u03B1\u03C4\u03B5
      \u03AD\u03BD\u03B1\u03BD \u03C0\u03B9\u03B8\u03B1\u03BD\u03CC \u03C0\u03B5\u03BB\u03AC\u03C4\u03B7
      \u03C3\u03C4\u03B7\u03BD \u03B5\u03C4\u03B1\u03B9\u03C1\u03B5\u03AF\u03B1 \u03C3\u03B1\u03C2.
      \u0391\u03BA\u03CC\u03BC\u03B1 \u03BA\u03B9 \u03B1\u03BD \u03B7 \u03B5\u03C0\u03B1\u03C6\u03AE
      \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B4\u03B9\u03BA\u03AE \u03C3\u03B1\u03C2
      \u03C0\u03B9\u03C3\u03C4\u03B5\u03CD\u03B5\u03C4\u03B5 \u03CC\u03C4\u03B9 \u03BC\u03CC\u03BD\u03BF
      \u03BF\u03B9 \u03B1\u03BD\u03CE\u03C4\u03B5\u03C1\u03BF\u03B9 \u03B5\u03C0\u03B1\u03B3\u03B3\u03B5\u03BB\u03BC\u03B1\u03C4\u03AF\u03B5\u03C2
      \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03C0\u03B1\u03C1\u03B5\u03C5\u03C1\u03B5\u03B8\u03BF\u03CD\u03BD
      \u03C3\u03C4\u03B7\u03BD \u03C0\u03C1\u03CE\u03C4\u03B7 \u03C3\u03C5\u03BD\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7
      \u03CC\u03C7\u03B9 \u03B5\u03C3\u03B5\u03AF\u03C2."
    - "\u039A\u03AC\u03C4\u03B9 \u03BD\u03AD\u03BF \u03C0\u03BB\u03B7\u03C3\u03B9\u03AC\u03B6\u03B5\u03B9:
      \u03AD\u03BD\u03B1 \u03BD\u03AD\u03BF \u03AD\u03C1\u03B3\u03BF \u03C0\u03C1\u03BF\u03CA\u03CC\u03BD
      \u03C5\u03C0\u03B7\u03C1\u03B5\u03C3\u03AF\u03B1. \u039D\u03BF\u03BC\u03AF\u03B6\u03B5\u03C4\u03B5
      \u03CC\u03C4\u03B9 \u03B7 \u03B9\u03B4\u03B1\u03BD\u03B9\u03BA\u03AE \u03BF\u03BC\u03AC\u03B4\u03B1
