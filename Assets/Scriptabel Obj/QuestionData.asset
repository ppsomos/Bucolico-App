      \u03B8\u03B1 \u03C3\u03C5\u03BD\u03B4\u03AD\u03B1\u03C4\u03B5 \u03B1\u03C5\u03C4\u03AC
      \u03C4\u03B1 \u03B4\u03CD\u03BF \u03C0\u03AC\u03B8\u03B7 \u03BC\u03B5\u03C4\u03B1\u03BE\u03CD
      \u03C4\u03BF\u03C5\u03C2;"
    - "\u0395\u03AF\u03C3\u03C4\u03B5 \u03C8\u03C5\u03C7\u03BF\u03BB\u03CC\u03B3\u03BF\u03C2
      \u03BA\u03B1\u03B9 \u03BC\u03B5 \u03C4\u03BF lockdown \u03C4\u03BF\u03C5 Covid-19
      \u03B7 \u03BA\u03BB\u03B9\u03BD\u03B9\u03BA\u03AE \u03C3\u03B1\u03C2 \u03AD\u03C0\u03C1\u03B5\u03C0\u03B5
      \u03BD\u03B1 \u03BA\u03BB\u03B5\u03AF\u03C3\u03B5\u03B9 \u03B1\u03BB\u03BB\u03AC
      \u03BF\u03B9 \u03B1\u03C3\u03B8\u03B5\u03BD\u03B5\u03AF\u03C2 \u03C3\u03B1\u03C2
      \u03C7\u03C1\u03B5\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03B9 \u03C4\u03B7
      \u03B2\u03BF\u03AE\u03B8\u03B5\u03B9\u03AC \u03C3\u03B1\u03C2 \u03BA\u03B1\u03B9
      \u03C4\u03B7 \u03B8\u03B5\u03C1\u03B1\u03C0\u03B5\u03AF\u03B1. \u03A4\u03B9
      \u03BB\u03CD\u03C3\u03B5\u03B9\u03C2 \u03C0\u03C1\u03BF\u03C4\u03B5\u03AF\u03BD\u03B5\u03C4\u03B5;"
    - "\u0398\u03AD\u03BB\u03B5\u03C4\u03B5 \u03BD\u03B1 \u03B3\u03AF\u03BD\u03B5\u03C4\u03B5
      \u03B5\u03C0\u03B9\u03C7\u03B5\u03B9\u03C1\u03B7\u03BC\u03B1\u03C4\u03AF\u03B1\u03C2
      \u03BA\u03B1\u03B9 \u03C3\u03BA\u03AD\u03C6\u03C4\u03B5\u03C3\u03C4\u03B5 \u03BC\u03B9\u03B1
      \u03B5\u03BE\u03B1\u03B9\u03C1\u03B5\u03C4\u03B9\u03BA\u03AE \u03B9\u03B4\u03AD\u03B1
      \u03B3\u03B9\u03B1 \u03BC\u03B9\u03B1 \u03C5\u03C0\u03B7\u03C1\u03B5\u03C3\u03AF\u03B1.
      \u03A0\u03B9\u03C3\u03C4\u03B5\u03CD\u03B5\u03C4\u03B5 \u03B1\u03BA\u03C1\u03AC\u03B4\u03B1\u03BD\u03C4\u03B1
      \u03CC\u03C4\u03B9 \u03B8\u03B1 \u03AD\u03C7\u03B5\u03B9 \u03B5\u03C0\u03B9\u03C4\u03C5\u03C7\u03AF\u03B1
      \u03BA\u03B1\u03B9 \u03B5\u03AF\u03C3\u03C4\u03B5 \u03AD\u03C4\u03BF\u03B9\u03BC\u03BF\u03B9
      \u03BD\u03B1 \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03AE\u03C3\u03B5\u03C4\u03B5
      \u03BC\u03B9\u03B1 \u03B5\u03C4\u03B1\u03B9\u03C1\u03B5\u03AF\u03B1 \u03C0\u03BF\u03C5
      \u03B8\u03B1 \u03C0\u03B1\u03C1\u03AD\u03C7\u03B5\u03B9 \u03B1\u03C5\u03C4\u03AE
      \u03C4\u03B7\u03BD \u03C5\u03C0\u03B7\u03C1\u03B5\u03C3\u03AF\u03B1. \u03A4\u03B9
      \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5 \u03C0\u03C1\u03CE\u03C4\u03B1;"
    - "\u0388\u03C7\u03B5\u03C4\u03B5 15\u03B5\u03C4\u03AE \u03B5\u03BC\u03C0\u03B5\u03B9\u03C1\u03AF\u03B1
      \u03C3\u03C4\u03BF HR. \u0388\u03C7\u03B5\u03C4\u03B5 \u03B2\u03B1\u03C1\u03B5\u03B8\u03B5\u03AF
      \u03CC\u03BC\u03C9\u03C2 \u03C4\u03B7\u03BD \u03B5\u03C4\u03B1\u03B9\u03C1\u03B9\u03BA\u03AE
      \u03B6\u03C9\u03AE \u03BA\u03B1\u03B9 \u03B8\u03AD\u03BB\u03B5\u03C4\u03B5
      \u03BD\u03B1 \u03BE\u03B5\u03BA\u03B9\u03BD\u03AE\u03C3\u03B5\u03C4\u03B5 \u03BA\u03AC\u03C4\u03B9
      \u03BD\u03AD\u03BF \u03BA\u03B1\u03B9 \u03C3\u03C5\u03BD\u03B1\u03C1\u03C0\u03B1\u03C3\u03C4\u03B9\u03BA\u03CC.
      \u0395\u03BE\u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03B5\u03AF\u03C4\u03B5
      \u03BD\u03B1 \u03B8\u03AD\u03BB\u03B5\u03C4\u03B5 \u03BD\u03B1 \u03B4\u03B9\u03B1\u03C4\u03B7\u03C1\u03AE\u03C3\u03B5\u03C4\u03B5
      \u03C4\u03BF\u03BD \u03C4\u03C1\u03CC\u03C0\u03BF \u03B6\u03C9\u03AE\u03C2
      \u03C0\u03BF\u03C5 \u03AD\u03C7\u03B5\u03C4\u03B5 \u03C3\u03C5\u03BD\u03B7\u03B8\u03AF\u03C3\u03B5\u03B9
      \u03B5\u03C0\u03BF\u03BC\u03AD\u03BD\u03C9\u03C2 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
      \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BA\u03AC\u03C4\u03B9 \u03C0\u03BF\u03C5
      \u03C3\u03B1\u03C2 \u03B4\u03AF\u03BD\u03B5\u03B9 \u03B5\u03BB\u03B5\u03C5\u03B8\u03B5\u03C1\u03AF\u03B1
      \u03BA\u03B1\u03B9 \u03C7\u03C1\u03AE\u03BC\u03B1\u03C4\u03B1. \u03A4\u03B9
      \u03B5\u03C0\u03B9\u03BB\u03AD\u03B3\u03B5\u03C4\u03B5;"
    - "\u0388\u03C7\u03B5\u03C4\u03B5 \u03AD\u03BD\u03B1 \u03C0\u03C1\u03BF\u03CA\u03CC\u03BD
      \u03BC\u03B9\u03B1 \u03B9\u03C3\u03C4\u03BF\u03C3\u03B5\u03BB\u03AF\u03B4\u03B1
      \u03BC\u03B9\u03B1 \u03C0\u03B1\u03C1\u03BF\u03C5\u03C3\u03AF\u03B1 \u03C3\u03C4\u03B1
      \u03BC\u03AD\u03C3\u03B1 \u03BA\u03BF\u03B9\u03BD\u03C9\u03BD\u03B9\u03BA\u03AE\u03C2
      \u03B4\u03B9\u03BA\u03C4\u03CD\u03C9\u03C3\u03B7\u03C2. \u0393\u03C1\u03AC\u03C6\u03B5\u03C4\u03B5
      \u03B5\u03BB\u03BA\u03C5\u03C3\u03C4\u03B9\u03BA\u03AD\u03C2 \u03B1\u03BD\u03B1\u03C1\u03C4\u03AE\u03C3\u03B5\u03B9\u03C2
      \u03C0\u03C1\u03BF\u03C9\u03B8\u03B5\u03AF\u03C4\u03B5 \u03C4\u03BF \u03C0\u03C1\u03BF\u03CA\u03CC\u03BD
      \u03C3\u03B1\u03C2 \u03B1\u03BB\u03BB\u03AC \u03BA\u03B1\u03BD\u03B5\u03AF\u03C2
      \u03B4\u03B5\u03BD \u03C4\u03BF \u03B1\u03B3\u03BF\u03C1\u03AC\u03B6\u03B5\u03B9.
      \u03A4\u03B9 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5;"
    - "\u0395\u03AF\u03C3\u03C4\u03B5 \u03AD\u03BC\u03C0\u03B5\u03B9\u03C1\u03BF\u03C2
      \u03B4\u03B9\u03B5\u03C5\u03B8\u03C5\u03BD\u03C4\u03AE\u03C2 \u03BC\u03B5 \u03B5\u03C0\u03B9\u03B8\u03C5\u03BC\u03AF\u03B1
      \u03BD\u03B1 \u03B1\u03BD\u03B1\u03C0\u03C4\u03C5\u03C7\u03B8\u03B5\u03AF\u03C4\u03B5.
      \u0391\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03B5\u03AF\u03C4\u03B5 \u03C0\u03C1\u03BF\u03C0\u03BF\u03BD\u03B7\u03C4\u03B9\u03BA\u03AE
      \u03B5\u03BA\u03C0\u03B1\u03AF\u03B4\u03B5\u03C5\u03C3\u03B7 \u03B3\u03B9\u03B1
      \u03BD\u03B1 \u03B2\u03BF\u03B7\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5 \u03BA\u03B1\u03BB\u03CD\u03C4\u03B5\u03C1\u03B1
      \u03C4\u03B7\u03BD \u03BF\u03BC\u03AC\u03B4\u03B1 \u03C3\u03B1\u03C2 \u03BD\u03B1
      \u03B1\u03BD\u03B1\u03C0\u03C4\u03C5\u03C7\u03B8\u03B5\u03AF \u03BA\u03B1\u03B9
      \u03BD\u03B1 \u03B3\u03AF\u03BD\u03B5\u03C4\u03B5 \u03B5\u03B9\u03B4\u03B9\u03BA\u03CC\u03C2
      \u03C0\u03C1\u03BF\u03C0\u03BF\u03BD\u03B7\u03C4\u03AE\u03C2. \u0391\u03BD\u03B1\u03BA\u03B1\u03BB\u03CD\u03C0\u03C4\u03B5\u03C4\u03B5
      \u03CC\u03C4\u03B9 \u03C4\u03BF \u03BD\u03B1 \u03B2\u03BF\u03B7\u03B8\u03AC\u03C4\u03B5
      \u03C4\u03BF\u03C5\u03C2 \u03B1\u03BD\u03B8\u03C1\u03CE\u03C0\u03BF\u03C5\u03C2
      \u03BC\u03AD\u03C3\u03C9 \u03C4\u03B7\u03C2 \u03BA\u03B1\u03B8\u03BF\u03B4\u03AE\u03B3\u03B7\u03C3\u03B7\u03C2
      \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03BF \u03BA\u03AC\u03BB\u03B5\u03C3\u03BC\u03AC
      \u03C3\u03B1\u03C2 \u03BA\u03B1\u03B9 \u03C3\u03BA\u03AD\u03C6\u03C4\u03B5\u03C3\u03C4\u03B5
      \u03BD\u03B1 \u03C4\u03BF \u03B5\u03C0\u03B9\u03B4\u03B9\u03CE\u03BE\u03B5\u03C4\u03B5.
      \u0391\u03BB\u03BB\u03AC \u03C4\u03BF \u03CC\u03BD\u03B5\u03B9\u03C1\u03CC
      \u03C3\u03B1\u03C2 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BD\u03B1 \u03C7\u03C4\u03AF\u03C3\u03B5\u03C4\u03B5
      \u03C4\u03BF\u03BD \u03BA\u03CC\u03C3\u03BC\u03BF \u03C7\u03C9\u03C1\u03AF\u03C2
      \u03BA\u03BF\u03B9\u03BD\u03C9\u03BD\u03B9\u03BA\u03AC \u03CC\u03C1\u03B9\u03B1
      \u03CC\u03C0\u03BF\u03C5 \u03C4\u03B1 \u03BA\u03BF\u03C1\u03AF\u03C4\u03C3\u03B9\u03B1
      \u03B5\u03BD\u03B8\u03B1\u03C1\u03C1\u03CD\u03BD\u03BF\u03BD\u03C4\u03B1\u03B9
      \u03BD\u03B1 \u03B1\u03C3\u03C7\u03BF\u03BB\u03B7\u03B8\u03BF\u03CD\u03BD \u03BC\u03B5
      \u03C4\u03B7\u03BD \u03B5\u03C0\u03B9\u03C3\u03C4\u03AE\u03BC\u03B7 \u03BA\u03B1\u03B9
      \u03C4\u03B1 \u03B1\u03B3\u03CC\u03C1\u03B9\u03B1 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BD
      \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C7\u03BF\u03C1\u03B5\u03C5\u03C4\u03AD\u03C2
      \u03BC\u03C0\u03B1\u03BB\u03AD\u03C4\u03BF\u03C5. \u0391\u03BB\u03BB\u03AC
      \u03C6\u03BF\u03B2\u03AC\u03C3\u03C4\u03B5 \u03CC\u03C4\u03B9 \u03B4\u03B5\u03BD
      \u03C5\u03C0\u03AC\u03C1\u03C7\u03BF\u03C5\u03BD \u03C0\u03BF\u03BB\u03BB\u03AC
      \u03C7\u03C1\u03AE\u03BC\u03B1\u03C4\u03B1 \u03B5\u03BA\u03B5\u03AF \u03BA\u03B1\u03B9
      \u03B4\u03B5\u03BD \u03B8\u03B1 \u03BC\u03C0\u03BF\u03C1\u03AD\u03C3\u03B5\u03C4\u03B5
      \u03C0\u03BF\u03C4\u03AD \u03BD\u03B1 \u03B4\u03B9\u03B1\u03C4\u03B7\u03C1\u03AE\u03C3\u03B5\u03C4\u03B5
